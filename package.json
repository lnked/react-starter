{
  "name": "awesome-react-starter",
  "version": "2.3.5",
  "license": "MIT",
  "main": "index.js",
  "description": "React boilerplate, pwa, router",
  "scripts": {
    "clean": "rimraf dist",
    "clear:babel-cache": "rimraf -rf ./node_modules/.cache/babel-loader/*",
    "prebuild-config": "yarn run clean && yarn run lint && yarn run clear:babel-cache",
    "postdeploy": "gh-pages -d dist",
    "postrelease": "yarn size-limit",
    "build-config": "cross-env NODE_ENV=production ./node_modules/.bin/webpack --config webpack/production.config -p",
    "build": "yarn run build-config --opts assemply",
    "start:dll": "cross-env NODE_ENV=development ./node_modules/.bin/webpack --config webpack/dll.config",
    "release:dll": "cross-env NODE_ENV=production ./node_modules/.bin/webpack --config webpack/dll.config",
    "release": "yarn run build-config --opts release",
    "analyze": "yarn run build-config --opts analyze",
    "deploy": "yarn run build-config --opts deploy",
    "stats": "yarn run build-config --json --profile > stats.json",
    "start": "cross-env NODE_ENV=development ./node_modules/.bin/webpack-dev-server --config webpack/development.config --opts development --env development",
    "lint:scripts": "./node_modules/.bin/eslint ./src --ext .js,.jsx",
    "lint:styles": "./node_modules/.bin/stylelint 'src/**/*.{css,less,styl,scss,sass,sss}'",
    "lint:styled-components": "./node_modules/.bin/stylelint 'src/**/*.{ts,tsx,js,jsx}'",
    "lint:tslint": "./node_modules/.bin/tslint 'src/**/*.{ts,tsx}' --project ./tsconfig.json --config ./tslint.json -t verbose",
    "lint": "yarn run lint:scripts && yarn run lint:styles && yarn run lint:tslint",
    "lint:fix-scripts": "./node_modules/.bin/eslint ./src --ext .ts,.tsx,.js,.jsx --fix",
    "lint:fix-tslint": "./node_modules/.bin/tslint ./src/**/*.{ts,tsx} --fix",
    "lint:fix-styles": "./node_modules/.bin/stylelint ./src/**/*.{css,less,styl,scss,sass,sss} --fix",
    "lint:fix": "yarn run lint:fix-scripts && yarn run lint:fix-tslint && yarn run lint:fix-styles && yarn lint:styled-components",
    "test": "cross-env NODE_ENV=test jest",
    "test-watch": "yarn run test --watch --notify",
    "test-cover": "yarn run test --coverage",
    "coverage": "yarn run test-cover && opn coverage/lcov-report/index.html",
    "publish": "npm puublish --access=public",
    "example-build": "yarn release --env example --reset-cache"
  },
  "keywords": [
    "react"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "git update-index --again"
    }
  },
  "lint-staged": {
    "ignore": [
      "package.json"
    ],
    "linters": {
      "*.{ts,tsx,js,jsx}": [
        "./node_modules/.bin/eslint --max-warnings=0",
        "yarn run lint:fix",
        "git add"
      ],
      "*.(js|jsx)": [
        "cross-env NODE_ENV=development eslint --cache --format=node_modules/eslint-formatter-pretty",
        "git add"
      ],
      "*.(json|md)": [
        "git add"
      ],
      "*.{css,less,styl,scss,sass,sss}": [
        "stylelint --fix",
        "git add --force"
      ]
    }
  },
  "repository": {
    "url": "git+https://github.com/lnked/react-template.git",
    "type": "git"
  },
  "author": "Edik Bulikyan <ed.proff@gmail.com>",
  "size-limit": [
    {
      "path": [
        "dist/**/*.{css,js,html,svg,png}"
      ],
      "limit": "500 KB",
      "webpack": false
    }
  ],
  "config": {
    "port": "3000"
  },
